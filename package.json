    "rimraf": "2.6.1",
    "rxjs-tslint": "0.1.7",
    "tslint": "5.11.0",
    "codelyzer": "5.1.0",
    "typescript": "3.4.5",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.4"
