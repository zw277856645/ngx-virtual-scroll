{
  "name": "@demacia/ngx-virtual-scroll",
  "version": "0.0.7",
  "description": "angular big list viewer",
  "scripts": {
    "build": "ng-packagr -p package.json",
    "publish": "npm publish dist --access public",
    "clean": "rimraf dist",
    "doc": "docsify serve docs --open",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "cssUrl": "inline"
    },
    "whitelistedNonPeerDependencies": [
      "@tweenjs/tween.js",
      "@types/tween.js",
      "@demacia/cmjs-lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/zw277856645/ngx-virtual-scroll"
  },
  "keywords": [
    "angular",
    "ngx",
    "virtual",
    "scroll",
    "scrolling",
    "scroller",
    "big list"
  ],
  "author": "wei.zhang",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^8.1.2",
    "@angular/common": "^8.1.2",
    "@angular/animations": "^8.1.2",
    "zone.js": "^0.9.1",
    "rxjs": "^6.5.1"
  },
  "dependencies": {
    "@tweenjs/tween.js": "~17.2.0",
    "@types/tween.js": "~16.9.0",
    "@demacia/cmjs-lib": "~0.0.1"
  },
  "devDependencies": {
    "@angular/common": "8.1.2",
    "@angular/core": "8.1.2",
    "@angular/animations": "8.1.2",
    "@angular/platform-browser": "8.1.2",
    "@angular/platform-browser-dynamic": "8.1.2",
    "@angular/compiler-cli": "8.1.2",
    "@angular/compiler": "8.1.2",
    "@types/node": "10.14.5",
    "zone.js": "0.9.1",
    "rxjs": "6.5.1",
    "ng-packagr": "5.1.0",
    "ajv": "6.9.1",
    "rimraf": "2.6.1",
    "rxjs-tslint": "0.1.7",
    "tslint": "5.11.0",
    "codelyzer": "5.1.0",
    "typescript": "3.4.5"
  }
}
