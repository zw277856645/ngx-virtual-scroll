[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=1

[{*.ng,*.sht,*.html,*.shtm,*.shtml,*.htm}]
indent_style=space
indent_size=2

[{*.jhm,*.xslt,*.xul,*.rng,*.xsl,*.xsd,*.ant,*.tld,*.fxml,*.jrxml,*.xml,*.jnlp,*.wsdl}]
indent_style=space
indent_size=4

[{.babelrc,.prettierrc,.stylelintrc,.eslintrc,jest.config,*.json,*.jsb3,*.jsb2,*.bowerrc}]
indent_style=space
indent_size=2

[*.less]
indent_style=space
indent_size=2

[*.js]
indent_style=space
indent_size=4

[{testcase,test-plan-trigger,verification-result,overall-capacity-model.ts,*.ats,*.ts,*.module,*.service}]
indent_style=space
indent_size=4

[{tsconfig.spec.json,tsconfig.app.json,tsconfig.json,tsconfig.e2e.json}]
indent_style=space
indent_size=2

[{.analysis_options,*.yml,*.yaml}]
indent_style=space
indent_size=2

[*.md]
trim_trailing_whitespace = false